		<url-pattern>/appCateServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CountServlet</servlet-name>
		<servlet-class>cn.ctdss.lr.servlet.CountServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CountServlet</servlet-name>
		<url-pattern>/countServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Createdata</servlet-name>
		<servlet-class>cn.ctdss.lr.servlet.Createdata</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Createdata</servlet-name>
		<url-pattern>/Createdata</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<context-param>
		<param-name>listings</param-name>
		<param-value>true</param-value>
	</context-param>
	<servlet>
		<description></description>
		<display-name>TrafficFlow</display-name>
		<servlet-name>TrafficFlow</servlet-name>
		<servlet-class>cn.ctdss.lr.servlet.TrafficFlow</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TrafficFlow</servlet-name>
		<url-pattern>/TrafficFlow</url-pattern>
	</servlet-mapping>
</web-app>